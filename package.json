{
  "name": "artistaphoto",
  "version": "1.0.0",
  "description": "Professional image editor SDK for browser with filters, adjustments, crop, resize, text, shapes and more",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./worker": "./dist/worker.js"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "image",
    "editor",
    "canvas",
    "filter",
    "photo",
    "browser",
    "sdk",
    "typescript",
    "image-processing",
    "photo-editor",
    "crop",
    "resize",
    "filters",
    "adjustments"
  ],
  "author": "ArtistAPhoto",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://artistaphoto.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/artistaphoto/artistaphoto-sdk"
  },
  "bugs": {
    "url": "https://github.com/artistaphoto/artistaphoto-sdk/issues"
  },
  "devDependencies": {
    "@vitest/ui": "^2.1.9",
    "jsdom": "^25.0.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  }
}
